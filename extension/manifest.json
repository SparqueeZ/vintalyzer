{
    "manifest_version": 3,
    "name": "Vintalyze",
    "version": "1.0.0",
    "description": "Analyseur intelligent de profils et boutiques Vinted",
    "permissions": [
        "activeTab",
        "scripting",
        "clipboardWrite"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "https://www.vinted.fr/*"
    ]
}
